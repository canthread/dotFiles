[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  move_tab_right = <Primary><Shift>l
  move_tab_left = <Primary><Shift>h
  next_tab = <Primary>l
  prev_tab = <Primary>h
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
